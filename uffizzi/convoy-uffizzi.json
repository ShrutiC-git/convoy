{
    "environment": "dev",
    "queue": {
        "type": "redis",
        "redis": {
            "dsn": "redis://localhost:6379"
        }
    },
    "cache": {
        "type": "redis",
        "redis": {
            "dsn": "redis://localhost:6379"
        }
    },
    "host": "localhost:5005",
    "logger": {
        "level": "fatal"
    },
    "server": {
        "http": {
            "ssl": false,
            "ssl_cert_file": "",
            "ssl_key_file": "",
            "port": 5005
        }
    },
    "database": {
        "type": "mongo",
        "dsn": "mongodb://127.0.0.1:27017/convoy?replicaSet=localhost"
    },
    "group": {
        "strategy": {
            "type": "default",
            "default": {
                "intervalSeconds": 125,
                "retryLimit": 15
            }
        },
        "signature": {
            "header": "X-Company-Event-WebHook-Signature",
            "hash": "SHA256"
        }
    }
}