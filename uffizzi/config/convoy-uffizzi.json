{
    "queue": {
        "type": "redis",
        "redis": {
            "dsn": "redis://localhost:6379"
        }
    },
    "host": "localhost:8080",
    "database": {
        "type": "mongo",
        "dsn": "mongodb://127.0.0.1:27017,127.0.0.1:27019/convoy?replicaSet=localhost"
    },
    "auth": {
        "require_auth": true,
        "file": {
            "basic": [
                {
                    "username": "123",
                    "password": "abc",
                    "role": {
                        "type": "admin",
                        "project": ""
                    }
                }
            ]
        }
    },
    "group": {
        "strategy": {
            "type": "default",
            "default": {
                "intervalSeconds": 125,
                "retryLimit": 15
            }
        },
        "signature": {
            "hash": "SHA256"
        }
    }
}
